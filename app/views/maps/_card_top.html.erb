
<% @districts.last(3).each do |district| %>
<div class="row">
<div class="card-trip">
  <div class="card-trip-infos">
    <div>
      <p><%= district[:name] %></p>
      <% p district %>
      <p class="score"><%= (district[:average] / (params[:criteria].length - 1)) %></p>
    </div>
  </div>
</div>
</div>
<% end %>
